{
  "name": "VerdeOrganiko",
  "version": "1.0.0",
  "description": "organic tropical plants",
  "main": "server.js",
  "engines": {
    "node": "v12.18.0"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Geovany17/VerdeOrganiko2"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Geovany17/VerdeOrganiko2/issues"
  },
  "homepage": "https://github.com/Geovany17/VerdeOrganiko2#readme",
  "dependencies": {
    "axios": "^0.21.0",
    "concurrently": "^5.3.0",
    "bcryptjs": "^2.4.3",
    "crypto": "^1.0.1",
    "dotenv": "^8.0.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "validatorjs": "^3.18.1",
    "express": "^4.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.10.12"
  }
}
